pipeline {
    agent any
    tools {
        maven 'Maven'
        jdk 'JDK11'
    }
    stages{
        stage('git checkout'){
            steps{
            git branch: 'main', url: 'https://github.com/abhishekhg1749/firstdevopsrepo.git'
            }
        }
    
        stage('maven clean'){
            steps{
            sh 'mvn clean'
        }

        }
        stage('maven build'){
            steps{
            sh 'mvn install'
            } 
        }
        stage('depoly'){
            steps{
            echo"deploying the application"                
            }
        }
        stage('test'){
            steps{
            echo"testing the application"                
            }
        }
    }
}
